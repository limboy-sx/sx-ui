#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/SHENXIAN/11/VUE3ts/5.组件库开发/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_d83fb0b2b40228bde360310605a95b5d/node_modules/vitepress/bin/node_modules:/mnt/d/SHENXIAN/11/VUE3ts/5.组件库开发/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_d83fb0b2b40228bde360310605a95b5d/node_modules/vitepress/node_modules:/mnt/d/SHENXIAN/11/VUE3ts/5.组件库开发/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_d83fb0b2b40228bde360310605a95b5d/node_modules:/mnt/d/SHENXIAN/11/VUE3ts/5.组件库开发/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/SHENXIAN/11/VUE3ts/5.组件库开发/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_d83fb0b2b40228bde360310605a95b5d/node_modules/vitepress/bin/node_modules:/mnt/d/SHENXIAN/11/VUE3ts/5.组件库开发/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_d83fb0b2b40228bde360310605a95b5d/node_modules/vitepress/node_modules:/mnt/d/SHENXIAN/11/VUE3ts/5.组件库开发/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_d83fb0b2b40228bde360310605a95b5d/node_modules:/mnt/d/SHENXIAN/11/VUE3ts/5.组件库开发/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
